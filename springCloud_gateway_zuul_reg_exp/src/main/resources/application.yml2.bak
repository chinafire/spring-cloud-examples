spring:
  application:
    name: springCloud-gateway-zuul

server:
  port: 8111
  
eureka: 
  client: 
    service-url: 
      defaultZone: http://user:password123@localhost:8761/eureka
  instance:
    prefer-ip-address: true


zuul:
  #忽略反向代理服务（自定义配置的服务除外）
#  ignoredServices: '*'
#  ignoredServices: springcloud-provider-ribbon-hystrix

  #自定义服务名称
  routes:
    abc:
      path: /user-path/**
      serviceId: springcloud-provider