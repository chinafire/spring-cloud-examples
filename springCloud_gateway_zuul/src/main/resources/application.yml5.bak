spring:
  application:
    name: springCloud-gateway-zuul

server:
  port: 8111
  
eureka: 
  client: 
    service-url: 
      defaultZone: http://user:password123@localhost:8761/eureka
  instance:
    prefer-ip-address: true

#访问网关的前缀
zuul:
  prefix: /api
  strip-prefix: false
logging:
  level:
    com.netflix: debug

